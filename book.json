{
    "title": "一个嵌入式操作系统的实现过程",
    "language": "zh-hans",
    "plugins": [
        "katex",
        "prism",
        "prism-themes",
        "-highlight",
        "include-codeblock",
        "ace",
        "splitter",
        "chapter-fold",
        "copy-code-button",
        "-sharing",
        "sharing-plus"],

    "pluginsConfig": {
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "theme": "monokai"
        },
        "sharing": {
             "douban": true,
             "qq": true,
             "qzone": true,
             "weibo": true,
          "all": [
               "douban", "weibo",
               "qq", "qzone"
           ]
       }
    }
}
